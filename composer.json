{
    "name": "linkdup/silverstripe-googlemapfield",
    "type": "silverstripe-vendormodule",
    "description": "Save locations using latitude/longitude DataObject fields.",
    "keywords": [
        "maps",
        "google",
        "googlemapfield",
        "silverstripe",
        "map",
        "geolocation"
    ],
    "homepage": "http://github.com/BetterBrief/googlemapfield",
    "license": "BSD",
    "authors": [
        {
            "name": "Will Morgan",
            "homepage": "http://twitter.com/willmorgan",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/framework": "^4 || ^5"
    },
    "autoload": {
        "psr-4": {
            "BetterBrief\\": "src/"
        }
    },
    "replace": {
        "betterbrief/silverstripe-googlemapfield": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/css",
            "client/js"
        ]
    }
}
